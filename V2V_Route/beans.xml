<bean id="system_control", class="system_control">
</bean>

<bean id="global_control_config", class="global_control_config">
	<property name="platform", value="Windows"/>
	<property name="ntti", value="1000"/>
	<property name="gtt_mode", value="HIGHSPEED"/>
	<property name="route_mode", value="UDP"/>
</bean>



<bean id="gtt_highspeed_config", class="gtt_highspeed_config">
	<property name="road_length", value="3000"/>
	<property name="road_width", value="200"/>
	<property name="speed", value="120"/>
	<property name="freshtime", value="100"/>
</bean>

<bean id="gtt_urban_config", class="gtt_urban_config">
	<property name="road_length_ew", value="3000"/>
	<property name="road_length_sn", value="200"/>
	<property name="road_width", value="120"/>
	<property name="speed", value="120"/>
	<property name="freshtime", value="120"/>
</bean>

<bean id="rrm_config", class="rrm_config">
	<property name="total_bandwidth", value="3000"/>
	<property name="rb_num_per_pattern", value="200"/>
	<property name="drop_sinr_boundary", value="120"/>
	<property name="select_altorithm", value="120"/>
	<property name="time_division_granularity", value="120"/>
	
	<pre-processor method_name="set_pattern_num"/>
</bean>


<bean id="tmc_config", class="tmc_config">
	<property name="package_num", value="3000"/>
	<property name="hello_tti", value="200"/>
	<property name="trigger_rate", value="120"/>
</bean>


<bean id="route_config", class="route_config">
	<property name="interval", value="3000"/>
</bean>






